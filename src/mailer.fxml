<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.reclamation.MailerController">
    <stylesheets>
        <URL value="@mailer.css" />
    </stylesheets>
   <children>
