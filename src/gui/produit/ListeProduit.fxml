<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.produit.ListeProduitController">
   <children>
      <TableView fx:id="ListeProduits" layoutX="1.0" layoutY="44.0" prefHeight="356.0" prefWidth="518.0">
        <columns>
          <TableColumn fx:id="resImg" prefWidth="85.0" text="Image" />
          <TableColumn fx:id="resNom" onEditCommit="#ChangerNom" prefWidth="68.0" text="Nom" />
            <TableColumn fx:id="resDesc" onEditCommit="#ChangerDesc" prefWidth="88.0" text="Description" />
            <TableColumn fx:id="resQt" onEditCommit="#ChangerQt" prefWidth="77.0" text="Quantité" />
            <TableColumn fx:id="resPx" onEditCommit="#ChangerPx" prefWidth="94.0" text="Prix" />
            <TableColumn fx:id="resCat" prefWidth="105.0" text="Catégorie" />
        </columns>
      </TableView>
      <Label layoutX="211.0" layoutY="13.0" text="Liste des produits">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="ajouter" layoutX="518.0" layoutY="121.0" mnemonicParsing="false" onAction="#ajouter" prefHeight="25.0" prefWidth="82.0" text="Ajouter" />
      <Button fx:id="supprimer" layoutX="518.0" layoutY="200.0" mnemonicParsing="false" onAction="#supprimer" prefHeight="25.0" prefWidth="82.0" text="Supprimer" />
      <Button fx:id="front" layoutX="518.0" layoutY="265.0" mnemonicParsing="false" onAction="#front" prefHeight="25.0" prefWidth="82.0" text="Front" />
      <Button fx:id="GCat" layoutX="1.0" layoutY="14.0" mnemonicParsing="false" onAction="#Gcat" prefHeight="25.0" prefWidth="150.0" text="Gestion Catégorie" />
      <TextField fx:id="rechercher" layoutX="444.0" layoutY="14.0" />
   </children>
</AnchorPane>
