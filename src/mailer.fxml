      <TextField fx:id="subject" layoutX="153.0" layoutY="98.0" prefHeight="26.0" prefWidth="200.0" />
      <Label layoutX="46.0" layoutY="102.0" text="Subject" />
      <Label layoutX="197.0" layoutY="14.0" text="Mailing">
         <font>
            <Font size="34.0" />
         </font>
      </Label>
      <Button fx:id="send" layoutX="441.0" layoutY="348.0" mnemonicParsing="false" onAction="#sendi" text="Send" />
      <Button fx:id="R" layoutX="501.0" layoutY="348.0" mnemonicParsing="false" onAction="#retour" text="Return" />
   </children>
</AnchorPane>
